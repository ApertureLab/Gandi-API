{
  "name": "narno/gandi-api",
  "replace": {"narno/zendservice-gandi": "self.version"},
  "description": "Simple PHP library for the Gandi API.",
  "keywords": ["gandi", "api", "zf2"],
  "homepage": "https://project.aperturelab.fr/Gandi-API/",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Arnaud Ligny",
      "email": "arnaud@ligny.org",
      "homepage": "https://arnaudligny.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.4|^7.0",
    "zendframework/zend-xmlrpc": "^2.6"
  },
  "autoload": {
    "psr-4" : {
      "Narno\\Gandi\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
